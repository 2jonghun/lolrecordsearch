<div class="main__titlebox">
  <div class="main__logo">
    <a href="/"><img class="logo__lorec" src="/img/logo/lorec.svg" alt="logo"></a>
  </div>
</div>
<div class="main__searchbox">
  <div class="theme-mode">
    <!-- <div id="theme-dark" class="dark-mode">
      <i class="fa-solid fa-moon"></i>
    </div>
    <div id="theme-light" class="light-mode">
      <i class="fa-solid fa-sun"></i>
    </div> -->
  </div>
  <div class="main__inputbox">
    <select class="server__list">
      <% if (serverList) { %>
        <% for(let i = 0; i < serverList.length; i++) { %>
          <% if (!(serverList[i] === 'KR' || serverList[i] === 'RU')) { %>
            <% if (serverList[i] == latestServer) { %>
              <option value="<%= serverList[i] %>" selected><%= serverList[i].replace('1', '').toUpperCase() %></option>
            <% } else { %>
              <option value="<%= serverList[i] %>"><%= serverList[i].replace('1', '').toUpperCase() %></option>
            <% } %>
          <% } else { %>
            <% if (serverList[i] == latestServer) { %>
              <option value="<%= serverList[i] %>" selected><%= serverList[i].toUpperCase() %></option>
            <% } else { %>
              <option value="<%= serverList[i] %>"><%= serverList[i].toUpperCase() %></option>
            <% } %>
          <% } %>
        <% } %>
      <% } else { %>
        <option>not</option>
      <% } %> 
    </select>   
    <input type="text" class="username" placeholder="소환사명을 입력하세요">
    <button class="show_record_btn">
      <i class="fa-solid fa-magnifying-glass"></i>
    </button>
  </div>
</div>